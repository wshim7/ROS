#include <iostream>
#include <list>

using namespace std;

int main(int argc, char **argv) {

	int ar[] = {
			1, 2, 3
	};

	cout<<"address : "<<&ar[0]<<", "<<&ar[1]<<", "<<&ar[2]<<", "<<endl;

	list<int> l;
	l.push_back(1);
	l.push_back(2);
	l.push_back(3);


	for(int i = 0; i < l.size(); i++) {
		l.push_back(i);
	}

	return -1;
}


